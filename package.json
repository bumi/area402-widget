{
  "name": "area402-widget",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "source": "components/hello-world/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "build": "preact build --no-prerender --clean --template src/index.ejs --service-worker false",
    "serve": "preact serve",
    "dev": "preact watch --template src/index.ejs",
    "dist": "microbundle --entry src/components/hello-world/index.js"
  },
  "keywords": [],
  "author": "Zouhir <zouhir@zouhir.org>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "microbundle": "^0.12.4",
    "preact-cli": "^3.0.3",
    "preact-widget-scripts": "^0.6.0"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.0.0",
    "@emotion/css": "^11.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@emotion/styled-base": "^11.0.0",
    "preact": "^10.5.7",
    "preact-compat": "gist:developit/eeebf4c2449c9848dc95aff847d2454d",
    "preact-habitat": "^3.3.0",
    "preact-render-to-string": "^5.0.0",
    "preact-router": "^3.2.1",
    "qrcode.react": "^1.0.0",
    "stylis-plugin-extra-scope": "https://github.com/tpict/stylis-plugin-extra-scope.git#stylis-v4"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy",
      "^react$": "preact/compat",
      "^react-dom$": "preact/compat"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
